{
    "manifest_version": 2,
    "name": "untitled chrome interceptor",
    "description": "untitled chrome interceptor",
    "version": "0.1",
    "background": {
        "scripts": [
            "js/utils.js",
            "js/background.bundle.js"
        ]
    },
    "permissions": [
        "webRequest",
        "webRequestBlocking",
        "<all_urls>",
        "storage"
    ],
    "browser_action": {
        "default_popup": "pages/popup.html",
        "default_title": "untitled chrome interceptor"
    },
    "options_ui": {
        "page": "pages/options.html"
    }
}